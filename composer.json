  "keywords": ["laravel", "spark", "preset", "tailwindcss"],
  "license": "MIT",
  "require": {
    "laravel/framework": "^5.5 || ^6.0"
  },
  "autoload": {
    "psr-4": {
      "CentralityLabs\\SparkTailwindCssPreset\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "CentralityLabs\\SparkTailwindCssPreset\\SparkTailwindCssPresetServiceProvider"
      ]
    }
  }
}
